for(var month = 1; month<=12 ; month ++){

var tdate = 31 

       if(month == 2 ){
        
        tdate = 28 ;
     
       }
       else if(month==4 || month==6 || month==9 || month==11) {

         tdate = 30 ;
       }
       


for(var date = 1 ; date<=tdate; date++ ){

console.log (date +" -"+ month)

}



}
